    "@types/bun": "^1.1.3",
    "commitlint-rs-npm": "dalisoft/commitlint-rs-npm",
    "dprint": "^0.46.1",
    "typos-rs-npm": "dalisoft/typos-rs-npm"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/stream": "^1.0.2",
    "@elysiajs/swagger": "^1.0.5",
    "elysia": "^1.0.22",
    "tiktoken": "^1.0.15"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@evilmartians/lefthook",
    "commitlint-rs-npm",
