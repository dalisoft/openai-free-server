{
  "name": "openapi-free-server",
  "module": "src/server.ts",
  "type": "module",
  "scripts": {
    "pm2-start": "env $(cat .env) pm2 start bun --name \"api\" -- start",
    "dev": "bun --watch run src/server.ts",
